<mujoco model="arm2d2_gripper">
  <visual>
    <headlight ambient="0 0 0" diffuse="0.3 0.3 0.3" specular="1 1 1"/>
    <quality shadowsize="8192"/>
  </visual>
  <compiler angle="radian" meshdir="../meshes/" autolimits="true"/>
  <option integrator="RK4" timestep="0.0001" gravity="0 0 0">
    <!--<flag gravity="disable"/>-->
  </option>
  <size njmax="500" nconmax="100"/>

  <asset>
    <include file="gripper_assets.xml"/>
  </asset>

  <worldbody>
    <light pos=".4 .4 .5" dir="-2 -2 -1.5" diffuse="1 1 1"/>
    <geom type="plane" size="5 5 0.1" rgba="1 1 1 1"/>

    <body name="base" pos="0 0 0.1" euler="1.57 0 0">
      <geom type="box" size="0.001 0.001 0.001"/>
      <joint type="free"/>
      <include file="gripper_body.xml"/>
    </body>
  </worldbody>

  <include file="gripper_actuator.xml"/>

</mujoco>
  

